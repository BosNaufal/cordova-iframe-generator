<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bosnaufal.cordovaiframe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="www/icon.png" />
    <name>Cordova Iframe</name>
    <description>
        Cordova Focus On Iframe
    </description>
    <author email="bosnaufalemail@bosnaufal.com" href="http://github.com/BosNaufal">
        Naufal Rabbani
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <access origin="*" subdomains="true" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
